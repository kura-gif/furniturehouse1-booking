{
  "buildCommand": "npm run build",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "framework": "nuxtjs",
  "outputDirectory": ".output/public",
  "cleanUrls": true,
  "trailingSlash": false,
  "crons": [
    {
      "path": "/api/cron/send-reminders",
      "schedule": "0 0 * * *"
    },
    {
      "path": "/api/cron/send-thankyou",
      "schedule": "0 1 * * *"
    }
  ]
}
